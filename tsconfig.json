{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "wwwroot/scripts",
    "target": "ESNext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "importHelpers": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "sourceRoot": "/src"
  }
}
